<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <display-name>SEO BOX</display-name>
    <context-param>
        <param-name>jenkinsUrl</param-name>
        <param-value>http://10.207.16.9/jenkins/</param-value>
    </context-param>
    <context-param>
        <param-name>jenkinsJob</param-name>
        <param-value>SEOBOX</param-value>
    </context-param>
    <env-entry>
        <env-entry-name>dbHost</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>localhost</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>dbUsername</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>user</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>dbPassword</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>pass</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>dbPort</env-entry-name>
        <env-entry-type>java.lang.Integer</env-entry-type>
        <env-entry-value>27017</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>dbName</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>SEOBOX</env-entry-value>
    </env-entry>
    <servlet>
        <servlet-name>History</servlet-name>
        <servlet-class>com.seobox.archives.History</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HistoryPage</servlet-name>
        <jsp-file>/WEB-INF/pages/HistoryPage.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>Download</servlet-name>
        <servlet-class>com.seobox.archives.Download</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FetchResults</servlet-name>
        <servlet-class>com.seobox.services.FetchResults</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PageSpeedInsight</servlet-name>
        <servlet-class>com.seobox.services.PageSpeedInsight</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Demo</servlet-name>
        <servlet-class>com.seobox.services.Demo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StructuredData</servlet-name>
        <servlet-class>com.seobox.services.StructuredData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Build</servlet-name>
        <servlet-class>com.seobox.web.Build</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUploadServlet</servlet-name>
        <servlet-class>com.seobox.web.FileUploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Polling</servlet-name>
        <servlet-class>com.seobox.web.Polling</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>History</servlet-name>
        <url-pattern>/History</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HistoryPage</servlet-name>
        <url-pattern>/HistoryPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Download</servlet-name>
        <url-pattern>/Download</url-pattern>
        <url-pattern>/download</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FetchResults</servlet-name>
        <url-pattern>/FetchResults</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PageSpeedInsight</servlet-name>
        <url-pattern>/PageSpeedInsight</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Demo</servlet-name>
        <url-pattern>/Demo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StructuredData</servlet-name>
        <url-pattern>/StructuredData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Build</servlet-name>
        <url-pattern>/Build</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUploadServlet</servlet-name>
        <url-pattern>/FileUploadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Polling</servlet-name>
        <url-pattern>/Polling</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>60</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
